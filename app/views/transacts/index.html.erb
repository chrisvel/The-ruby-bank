<p id="notice"><%= notice %></p>

<h1>Listing Transacts</h1>

<table>
  <thead>
    <tr>
      <th>Amount</th>
      <th>From user</th>
      <th>To user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transacts.each do |transact| %>
      <tr>
        <td><%= transact.amount %></td>
        <td><%= transact.from_user_id %></td>
        <td><%= transact.to_user_id %></td>
        <td><%= link_to 'Show', transact %></td>
        <td><%= link_to 'Edit', edit_transact_path(transact) %></td>
        <td><%= link_to 'Destroy', transact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transact', new_transact_path %>
